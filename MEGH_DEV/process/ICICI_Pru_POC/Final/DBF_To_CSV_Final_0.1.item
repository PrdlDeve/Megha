  <node componentName="tJava" componentVersion="0.101" offsetLabelX="0" offsetLabelY="0" posX="560" posY="280">
    <elementParameter field="TEXT" name="UNIQUE_NAME" value="tJava_1" show="false"/>
    <elementParameter field="MEMO_JAVA" name="CODE" value="// Define the folder paths&#xA;String dbfFolderPath = &quot;C:/Users/Laptop_User/Desktop/Talend/Client/ICICI Pru/Job1_files/DBFZip/DBFZip&quot;;&#xA;String csvFolderPath = &quot;C:/Users/Laptop_User/Desktop/Talend/Client/ICICI Pru/Job1_files/DBFZip/DBFZip&quot;;&#xA;&#xA;// Create the output folder if it does not exis&#xA;File csvFolder = new File(csvFolderPath);&#xA;if (!csvFolder.exists()) {&#xA;    csvFolder.mkdirs();&#xA;}&#xA;&#xA;// Get the list of .dbf files in the folder&#xA;File dbfFolder = new File(dbfFolderPath);&#xA;File[] dbfFiles = dbfFolder.listFiles((dir, name) -> name.toLowerCase().endsWith(&quot;.dbf&quot;));&#xA;&#xA;if (dbfFiles != null) {&#xA;    for (File dbfFile : dbfFiles) {&#xA;        String dbfFilePath = dbfFile.getAbsolutePath();&#xA;        String csvFilePath = csvFolderPath + &quot;/&quot; + dbfFile.getName().replace(&quot;.dbf&quot;, &quot;.csv&quot;);&#xA;&#xA;        // Variables for file handling&#xA;        FileInputStream fis = null;&#xA;        DBFReader reader = null;&#xA;        PrintWriter writer = null;&#xA;&#xA;        try {&#xA;            // Initialize file input and reader&#xA;            fis = new FileInputStream(dbfFilePath);&#xA;            reader = new DBFReader(fis);&#xA;            writer = new PrintWriter(csvFilePath, StandardCharsets.UTF_8);&#xA;&#xA;            // Write CSV header&#xA;            int numberOfFields = reader.getFieldCount();&#xA;            for (int i = 0; i &lt; numberOfFields; i++) {&#xA;                DBFField field = reader.getField(i);&#xA;                writer.print(field.getName());&#xA;                if (i &lt; numberOfFields - 1) {&#xA;                    writer.print(&quot;,&quot;);&#xA;                }&#xA;            }&#xA;            writer.println();&#xA;&#xA;            // Write CSV rows&#xA;            Object[] rowObjects;&#xA;            NumberFormat numberFormat = NumberFormat.getInstance(Locale.US); // Ensure NumberFormat is imported and used&#xA;            while ((rowObjects = reader.nextRecord()) != null) {&#xA;                for (int i = 0; i &lt; rowObjects.length; i++) {&#xA;                    Object value = rowObjects[i];&#xA;                    if (value == null) {&#xA;                        writer.print(&quot;&quot;); // Write an empty string for null fields&#xA;                    } else {&#xA;                        String stringValue = value.toString();&#xA;                        // Handle numeric values&#xA;                        try {&#xA;                            if (stringValue.trim().isEmpty()) {&#xA;                                writer.print(&quot;&quot;); // Handle empty strings as empty&#xA;                            } else {&#xA;                                // Check if the value can be parsed as a number&#xA;                                try {&#xA;                                    writer.print(numberFormat.parse(stringValue).toString()); // Parse numeric values&#xA;                                } catch (ParseException ex) {&#xA;                                    writer.print(stringValue); // If parsing fails, print the string value&#xA;                                }&#xA;                            }&#xA;                        } catch (Exception e) {&#xA;                            writer.print(stringValue); // Print the string value if parsing fails&#xA;                        }&#xA;                    }&#xA;                    if (i &lt; rowObjects.length - 1) {&#xA;                        writer.print(&quot;,&quot;);&#xA;                    }&#xA;                }&#xA;                writer.println();&#xA;            }&#xA;        } catch (Exception e) {&#xA;            e.printStackTrace();&#xA;        } finally {&#xA;            // Close resources&#xA;            try {&#xA;                if (fis != null) {&#xA;                    fis.close();&#xA;                }&#xA;                if (writer != null) {&#xA;                    writer.close();&#xA;                }&#xA;            } catch (Exception e) {&#xA;                e.printStackTrace();&#xA;            }&#xA;        }&#xA;    }&#xA;} else {&#xA;    System.out.println(&quot;No .dbf files found in the folder: &quot; + dbfFolderPath);&#xA;}&#xA;"/>
    <elementParameter field="MEMO_IMPORT" name="IMPORT" value="import java.io.*;&#xA;import java.nio.charset.StandardCharsets;&#xA;import java.text.NumberFormat;&#xA;import java.text.ParseException;&#xA;import java.util.Locale;&#xA;import com.linuxense.javadbf.DBFField;&#xA;import com.linuxense.javadbf.DBFReader;"/>
    <elementParameter field="TEXT" name="CONNECTION_FORMAT" value="row"/>
